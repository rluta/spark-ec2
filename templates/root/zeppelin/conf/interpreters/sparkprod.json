{
  "id": "2BSEA6U9K",
  "name": "sparkprod",
  "group": "spark",
  "properties": {
    "spark.executor.memory": "1G",
    "spark.cores.max": "4",
    "spark.app.name": "Zeppelin-Prod"
  },
  "interpreterGroup": [
    {
      "class": "org.apache.zeppelin.spark.SparkInterpreter",
      "name": "spark"
    },
    {
      "class": "org.apache.zeppelin.spark.SparkSqlInterpreter",
      "name": "sql"
    },
    {
      "class": "org.apache.zeppelin.spark.DepInterpreter",
      "name": "dep"
    }
  ],
  "dependencies": [
    {
      "groupArtifactVersion": "/root/pipeline/lib/aldatalab_{{scala_version}}-1.3.2.jar",
      "local": false,
      "exclusions": [
        "org.scala-lang:scala-library",
        "org.apache.spark:spark-sql_{{scala_version}}"
      ]
    },
    {
      "groupArtifactVersion": "org.elasticsearch:elasticsearch-spark_{{scala_version}}:2.3.2",
      "local": false,
      "exclusions": [
        "org.scala-lang:scala-library",
        "org.apache.spark:spark-sql_{{scala_version}}"
      ]
    },
    {
      "groupArtifactVersion": "com.databricks:spark-csv_{{scala_version}}:1.4.0",
      "local": false,
      "exclusions": [
        "org.scala-lang:scala-library",
        "org.apache.spark:spark-sql_{{scala_version}}"
      ]
    },
    {
      "groupArtifactVersion": "info.debatty:java-string-similarity:0.13",
      "local": false,
      "exclusions": []
    }
  ],
  "option": {
    "remote": true,
    "perNoteSession": false,
    "perNoteProcess": false,
    "isExistingProcess": false
  }
}